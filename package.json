{
  "name": "np-cli",
  "description": "The NoProtocol Command Line Interface",
  "version": "0.1.0",
  "author": "Yaro Schiffelers",
  "bin": {
    "np": "./bin/run"
  },
  "main": "src/index.js",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.1",
    "@oclif/plugin-commands": "^1.2.2",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-plugins": "^1.7.8",
    "@oclif/plugin-update": "^1.3.9",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "aws-sdk": "^2.499.0",
    "inquirer": "^6.5.0",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "commitlint-circle": "^1.0.0",
    "globby": "^10",
    "husky": "^3.0.1",
    "mocha": "^5",
    "nyc": "^13",
    "standard-version": "^6.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "np",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ]
  },
  "homepage": "https://github.com/noprotocol/np-cli",
  "bugs": "https://github.com/noprotocol/np-cli/issues",
  "keywords": [
    "oclif",
    "cli",
    "np",
    "np-cli"
  ],
  "license": "MIT",
  "repository": "noprotocol/np-cli"
}
